@model eCinemaMS.Models.Account.ApplicationUser

@*Define ViewData*@
@{
    ViewData["Title"] = "List of Actors";   
}
@*Write HtmlCode*@


<div class="container">
     <div class="row">
        <div class="col-md-6">
            <h3 class="float-start">Profile </h3>
        </div>
    </div>
</div>
<hr/>
<div class="row">
    <div class="col-md-8 offset-md-2">
        <dl class="row">
            <dt class="col-sm-3 text-end">
                @Html.DisplayNameFor(Model => Model.FullName) : 
            </dt>
            <dd class="col-sm-9 text-start">
                @Html.DisplayFor(Model => Model.FullName)
            </dd>
            <dt class="col-sm-3 text-end">
                @Html.DisplayNameFor(Model => Model.Email) : 
            </dt>
            <dd class="col-sm-9 text-start">
                @Html.DisplayFor(Model => Model.Email)
            </dd>
            <dt class="col-sm-3 text-end">
                @Html.DisplayNameFor(Model => Model.PhoneNumber) : 
            </dt>
            <dd class="col-sm-9 text-start">
                @Html.DisplayFor(Model => Model.PhoneNumber)
            </dd>
            <dt class="col-sm-3 text-end">
                @Html.DisplayNameFor(Model => Model.CNIC) : 
            </dt>
            <dd class="col-sm-9 text-start">
                @Html.DisplayFor(Model => Model.CNIC)
            </dd>
             <dt class="col-sm-3 text-end">
                @Html.DisplayNameFor(Model => Model.EmailConfirmed) : 
            </dt>
            <dd class="col-sm-9 text-start">
                @Html.DisplayFor(Model => Model.EmailConfirmed)
            </dd>
        </dl>
    </div>
</div>
